<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <set android:ordering="sequentially">
        <objectAnimator
            android:duration="200"
            android:interpolator="@android:interpolator/linear"
            android:propertyName="pathData"
            android:valueFrom="@string/path_square_box_bottom_left"
            android:valueTo="@string/path_square_box_bottom_left"
            android:valueType="pathType" />
        <objectAnimator
            android:duration="300"
            android:interpolator="@interpolator/sephiroth_checkbox3_unchecked_mtrl_animation_interpolator_1"
            android:propertyName="pathData"
            android:valueFrom="@string/path_unchecked_to_checked_intermediate"
            android:valueTo="@string/path_checked"
            android:valueType="pathType" />
    </set>
    <set android:ordering="sequentially">
        <objectAnimator
            android:duration="166"
            android:interpolator="@android:interpolator/linear"
            android:propertyName="fillAlpha"
            android:valueFrom="0.0"
            android:valueTo="0.0" />
        <objectAnimator
            android:duration="33"
            android:interpolator="@interpolator/sephiroth_checkbox3_unchecked_mtrl_animation_interpolator_0"
            android:propertyName="fillAlpha"
            android:valueFrom="0.0"
            android:valueTo="1.0" />
    </set>
</set>
